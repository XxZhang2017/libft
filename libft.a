#include "libft.h"

void	*ft_memchr(const void *s, int c, size_t n)
{
	unsigned char	*ptr1;
	size_t	i;

	i = -1;
	ptr1 = s;
	while (++i < n)
	{
		if (ptr1[i] == (unsigned char)c)
			return (ptr1 + i + 1);
	}
	return (NULL);
}
