#include "libft.h"

void	*ft_memccpy(void *dst, const void *src, int c, size_t n)
{
	unsigned char	*ptr1;
	unsigned char	*ptr2;
	int	i;

	i = -1;
	ptr1 = (unsigned char *)dst;
	ptr2 = (unsigned char *)src;
	while (++i < n)
	{
		dst[i] = src[i];
		if (src[i] == (unsigned char)c)
			return (dst + i + 1);
	}
	return (NULL);
}

